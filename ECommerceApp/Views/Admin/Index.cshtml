@model ECommerceApp.Models.ApplicationUser
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Hello @Model.FirstName <span> </span> @Model.LastName </h2>

<table class="table table-bordered">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Birth Date</th>
        <th>Company</th>
       

    </tr>
    <tr>
    <td>@Model.FirstName</td>
    <td>@Model.LastName</td>
    <td>@Model.Email</td>
    <td>@Model.DaetOfBirth</td>
    <td>@Model.CompanyName</td>
  
</tr>
    
</table>